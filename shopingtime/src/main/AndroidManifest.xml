<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zerone.store.shopingtimetest">
    <!--需要添加权限判断 这个不是危险的所以不需要-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.zerone.store.com.zerone.shopingtimetest.Application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <!--登录页面的路由配置         -->
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.MainActivity"
            android:label="零壹零售"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.MakeSureTheOrderActivity" />
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.SystemSettingsActivity" />
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.TheOrderListActivity" />
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.OrderListActvity" />
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.OrderDetailsActivity" />
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.OrderDetailsYFKActivity" />
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.OrderDetailsYQXActivity" />
        <activity android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.ResultActivity" />
        <activity
            android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.SearchActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />

        <!--接收结果广播-->
        <receiver android:name="com.zerone.store.com.zerone.shopingtimetest.Activity.ResultReceiver">
            <intent-filter>
                <action android:name="sunmi.payment.L3.RESULT" />
            </intent-filter>
        </receiver>

        <!--&lt;!&ndash;接收结果广播&ndash;&gt;-->
        <!--<receiver android:name="com.zerone.store.shopingtime.Activity.GetPosInfoResultReceiver">-->
        <!--<intent-filter>-->
        <!--<action android:name="sunmi.payment.L3.RESULT" />-->
        <!--</intent-filter>-->
        <!--</receiver>-->
    </application>

</manifest>